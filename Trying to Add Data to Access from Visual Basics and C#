Public Class Form2
    Private Access As New dbcontrol

    Private Sub CheckBox1_CheckedChanged(sender As Object, e As EventArgs) Handles checkBox.CheckedChanged

    End Sub

    Private Sub Label3_Click(sender As Object, e As EventArgs) Handles noteLabel.Click

    End Sub

    Private Sub Label2_Click(sender As Object, e As EventArgs) Handles numberLabel.Click

    End Sub

    Private Sub TextBox3_TextChanged(sender As Object, e As EventArgs) Handles noteTxt.TextChanged

    End Sub

    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles clearCmd.Click
        nameTxt.Clear()
        numberTxt.Clear()
        noteTxt.Clear()

    End Sub
    Private Sub AddPart(PartName As String)
        'Functions for adding data
        Access.AddParam("@PartName", "%" & PartName & "%")
        Access.ExecQuery("INSERT INTO PartLists
                           PartName VALUES @PartName")


    End Sub
    Private Sub nameTxt_TextChanged(sender As Object, e As EventArgs) Handles nameTxt.TextChanged

    End Sub

    Private Sub addCmd_Click(sender As Object, e As EventArgs) Handles addCmd.Click
        'Calling Function
        AddPart(nameTxt.Text)
    End Sub
End Class
